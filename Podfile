  use_frameworks!
  # pod 'pjsip', '>= 2.6'
  pod 'pjsip', :git => 'https://github.com/chebur/pjsip.git', :commit => '0bd3ff6'
  # Pods for KHPhoneConnect

  target 'KHPhoneConnectTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
